{
    "name": "First",
    "version": "1.0",
    "manifest_version": 2,
    "description": "First",
    "icons": {
        "16": "images/icon-16.png", 
        "32": "images/icon-19.png" ,
        "48": "images/icon-38.png",
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_icon": {
            "16": "images/icon-16.png", 
            "32": "images/icon-19.png" ,
            "48": "images/icon-38.png",
            "128": "images/icon-128.png"
        },
        "default_popup": "popup.html"
    },
    "background": {
       "scripts": [
           "background.js"
       ],
       "persistent": false
   },
    "chrome_url_overrides":{
        
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "activeTab",
        "storage",
        "*://*/*",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
          "matches": ["*://*/*","https://www.youtube.com/","http://*/*", "https://*/*"],
          "js":["content.js"],
          "all_frames": true
        }
      ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}